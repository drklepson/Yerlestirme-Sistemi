{
  "hosting": {
    "site": "sahubot",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }],
    "redirects": [ {
      "source": "/auth",
      "destination": "/",
      "type": 301
    }]
  }
}
